# General setup
CONFIG_LOCALVERSION_AUTO=y

# Android
# CONFIG_ANDROID_LOW_MEMORY_KILLER is not set

# Compression
CONFIG_CRYPTO_LZ4=y

# CPU Frequency scaling
CONFIG_CPU_FREQ_STAT=y

# CPU/Task time and stats accounting
CONFIG_PSI=y

# DOS/FAT/EXFAT/NT Filesystems
CONFIG_SDFAT_FS=y
CONFIG_FAT_DEFAULT_IOCHARSET="utf8"
CONFIG_SDFAT_SUPPORT_DIR_SYNC=y
CONFIG_SDFAT_CHECK_RO_ATTR=y
CONFIG_SDFAT_VIRTUAL_XATTR=y
# CONFIG_SDFAT_DEBUG is not set

# IO Schedulers
CONFIG_CFQ_GROUP_IOSCHED=y

# Input Device Drivers
# CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_v26 is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_DSX is not set
CONFIG_INPUT_FINGERPRINT=y

# LED driver for blink(1) USB RGB LED is under Special HID drivers (HID_THINGM)
CONFIG_LEDS_AW2013=y
CONFIG_LEDS_AW2023=y

# Pseudo filesystems
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
# CONFIG_NLS_CODEPAGE_437 is not set
# CONFIG_NLS_ISO8859_1 is not set
CONFIG_NLS_UTF8=y

# Qualcomm Atheros Prima WLAN module
CONFIG_PRONTO_WLAN=y

# Qualcomm Technologies, Inc. MSM Camera And Video
CONFIG_MSMB_CAMERA_LEGACY=y

# RCU Subsystem
CONFIG_BLK_CGROUP=y
CONFIG_MEMCG=y
CONFIG_MEMCG_SWAP=y
